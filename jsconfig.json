{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@comp/*": ["src/components"],
      "@const/*": ["src/constants"],
      "@lang/*": ["src/lang"],
      "@redux/*": ["src/redux"],
      "@routes/*": ["src/routes"],
      "@screen/*": ["src/screen"],
      "@theme/*": ["src/theme"],
      "@common/*": ["src/common"],
      "@api/*": ["src/api"],
    }
  },
  "exclude": ["node_modules"]
}